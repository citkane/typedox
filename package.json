{
	"name": "typedox",
	"version": "0.0.0",
	"description": "Document Generator for Typedoc",
	"main": "./dist/index.js",
	"scripts": {
		"build:watch": "npx tsc -b -w",
		"backend": "node -r source-map-support/register ./dist/backend/index.js",
		"typedoc": "npx typedoc --tsconfig test/scenarios/testNamespaces/tsconfig.json test/scenarios/testNamespaces/index.ts"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/citkane/typedox.git"
	},
	"keywords": [
		"typescript",
		"tsdoc",
		"documentation",
		"generator",
		"frontend"
	],
	"author": "Michael Jonker",
	"license": "GPL-3.0-or-later",
	"bugs": {
		"url": "https://github.com/citkane/typedox/issues"
	},
	"homepage": "https://github.com/citkane/typedox#readme",
	"workspaces": [
		"./.tsconfig"
	],
	"devDependencies": {
		"@typedox/tsconfig": "*",
		"@types/fs-extra": "^11.0.1",
		"@types/node": "^20.5.6",
		"prettier": "^3.0.1",
		"source-map-support": "^0.5.21",
		"typedoc": "^0.24.8",
		"typescript": "^5.1.6"
	},
	"dependencies": {
		"@microsoft/api-extractor": "^7.36.3",
		"@microsoft/api-extractor-model": "^7.27.5",
		"@microsoft/tsdoc": "^0.14.2",
		"fs-extra": "^11.1.1"
	}
}
