{
	"name": "typedox",
	"version": "0.0.0",
	"description": "Document Generator for Typedoc",
	"main": "./dist/index.js",
	"scripts": {
		"build:watch": "npx tsc -b -w",
		"backend": "node -r source-map-support/register ./dist/backend/index.js",
		"help": "node dist/backend/index.js --help",
		"typedoc": "npx typedoc --tsconfig tsconfig.json src/backend/typedox.ts",
		"scenario:namespace:recursion": "node -r source-map-support/register ./dist/backend/index.js --tsConfigs test/scenarios/namespaces"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/citkane/typedox.git"
	},
	"keywords": [
		"typescript",
		"tsdoc",
		"documentation",
		"generator",
		"frontend"
	],
	"author": "Michael Jonker",
	"license": "GPL-3.0-or-later",
	"bugs": {
		"url": "https://github.com/citkane/typedox/issues"
	},
	"homepage": "https://github.com/citkane/typedox#readme",
	"workspaces": [
		"./.tsconfig"
	],
	"devDependencies": {
		"@typedox/tsconfig": "*",
		"@types/node": "^20.5.6",
		"prettier": "^3.0.1",
		"source-map-support": "^0.5.21",
		"typedoc": "^0.25.1"
	},
	"dependencies": {
		"typescript": "^5.2.2"
	}
}
