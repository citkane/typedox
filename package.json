		"packages/backend"
	],
	"devDependencies": {
		"@typedox/backend": "file:packages/backend",
		"@types/chai": "^4.3.6",
		"@types/mocha": "^10.0.1",
		"@types/sinon": "^10.0.16",
		"c8": "^8.0.1",
		"chai": "^4.3.8",
		"chai-exclude": "^2.1.0",
